{
  "name": "Thrive",
  "displayName": "Thrive",
  "expo": {
    "name": "Thrive",
    "slug": "thrive",
    "scheme": "thrive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/thrive-icon.png",
    "splash": {
      "image": "./assets/images/thrive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#191015"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/images/thrive-icon.png",
      "package": "com.happyporch.thrive",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/thrive-icon.png",
        "backgroundImage": "./assets/images/thrive-icon.png"
      },
      "splash": {
        "image": "./assets/images/thrive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "ios": {
      "icon": "./assets/images/thrive-icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.happyporch.thrive",
      "splash": {
        "image": "./assets/images/thrive-icon.png",
        "tabletImage": "./assets/images/thrive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "web": {
      "favicon": "./assets/images/thrive-icon.png",
      "splash": {
        "image": "./assets/images/thrive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "b23de2d3-ac76-4320-8aa2-7c306926afdd"
      }
    },
    "owner": "happyporch"
  }
}
