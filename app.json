{
  "name": "WildlifeApp",
  "displayName": "WildlifeApp",
  "expo": {
    "name": "WildlifeApp",
    "slug": "WildlifeApp",
    "scheme": "wildlifeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/thrive-icon.png",
    "splash": {
      "image": "./assets/images/thrive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#191015"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/images/thrive-icon.png",
      "package": "com.wildlifeapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/thrive-icon.png",
        "backgroundImage": "./assets/images/thrive-icon.png"
      },
      "splash": {
        "image": "./assets/images/thrive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "ios": {
      "icon": "./assets/images/thrive-icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.wildlifeapp",
      "splash": {
        "image": "./assets/images/thrive-icon.png",
        "tabletImage": "./assets/images/thrive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "web": {
      "favicon": "./assets/images/thrive-icon.png",
      "splash": {
        "image": "./assets/images/thrive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
